<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.PendingDataPointsController">
    <children>
        <TableView fx:id="table" layoutX="20.0" layoutY="65.0" prefHeight="200.0" prefWidth="561.0">
            <columns>
                <TableColumn prefWidth="57.0" text="Select" />
                <TableColumn maxWidth="159.0" minWidth="100.0" prefWidth="159.0" resizable="false" text="POI Location" />
                <TableColumn maxWidth="142.0" minWidth="92.0" prefWidth="97.0" text="Data Type" />
                <TableColumn prefWidth="114.0" text="Data Value" />
                <TableColumn prefWidth="136.0" text="Time and Date" />
            </columns>
        </TableView>
        <Label layoutX="135.0" layoutY="14.0" text="Pending Data Points" textFill="#b4b93c">
            <font>
                <Font name="Gill Sans MT Bold" size="29.0" />
            </font></Label>
        <Button fx:id="Back" layoutX="103.0" layoutY="330.0" mnemonicParsing="false" onAction="#handleBackPressed" text="Back" />
        <Button fx:id="Accept" layoutX="273.0" layoutY="330.0" mnemonicParsing="false" onAction="#handleAcceptPressed" text="Accept" />
        <Button fx:id="Reject" layoutX="452.0" layoutY="330.0" mnemonicParsing="false" onAction="#handleRejectPressed" text="Reject" />
    </children>
</AnchorPane>
